{
  "name": "streetdiariesproductions",
  "version": "1.0.0",
  "description": "A web application for a clothing store",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "clean": "rimraf dist",
    "build:client": "cd client && npm install && npm run build",
    "build:server": "tsc -p .",
